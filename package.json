{
  "name": "react-async-loader",
  "version": "0.1.2",
  "description": "Async script loading composition",
  "main": "modules/AsyncLoad.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build:commonjs": "babel src --out-dir modules",
    "build:umd": "NODE_ENV=development webpack src/index.js dist/react-async-loader.js",
    "build:umd:min": "NODE_ENV=production webpack src/index.js dist/react-async-loader.min.js",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "example": "webpack-dev-server --config webpack-example.config.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EdStudio/react-async-loader.git"
  },
  "keywords": [
    "React",
    "Async",
    "Script Loader"
  ],
  "author": "Edmund Hung <edmund@edstud.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EdStudio/react-async-loader/issues"
  },
  "homepage": "https://github.com/EdStudio/react-async-loader",
  "peerDependencies": {
    "react": ">=0.13"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-core": "^5.7.4",
    "babel-eslint": "^4.0.0",
    "babel-loader": "^5.3.2",
    "eslint": "^0.24.1",
    "eslint-plugin-react": "^3.0.0",
    "node-libs-browser": "^0.5.2",
    "react-hot-loader": "^1.2.8",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "hoist-non-react-statics": "^1.0.3"
  }
}
